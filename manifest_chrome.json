{
    "name": "NRDBlocker",
    "description": "Block Newly registered domain (NRD) for prevent threats",
    "version": "1.1",
    "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_title": "NRDBlocker",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "type": "module"
    },
    "permissions":["storage", "unlimitedStorage", "webRequest", "webRequestBlocking", "<all_urls>"]
}
